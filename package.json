{
  "name": "qi-credit",
  "version": "0.1.0",
  "description": "",
  "main": "backend/server.js",
  "scripts": {
    "start": "node scripts/start.js",
    "validate": "node scripts/validate-all.js",
    "start:backend": "cd backend && npm run dev",
    "start:frontend": "cd frontend && npm run dev",
    "dev": "node scripts/start.js",
    "setup:env": "node scripts/setup-env.js",
    "setup": "npm install && cd backend && npm install && cd ../frontend && npm install && cd ../blockchain && npm install && cd .. && npm run db:migrate",
    "build": "cd frontend && npm run build && cd ../backend && echo 'Backend build complete'",
    "lint": "cd frontend && npm run lint && cd ../backend && npm run lint",
    "type-check": "cd frontend && npm run type-check",
    "clean": "cd frontend && npm run clean && rm -rf node_modules/.cache",
    "test": "cd backend && npm test && cd ../blockchain && npm test",
    "deploy:contracts": "cd blockchain && npm run deploy:sepolia",
    "db:migrate": "cd backend && npx knex migrate:latest --knexfile ../database/knexfile.js",
    "db:rollback": "cd backend && npx knex migrate:rollback --knexfile ../database/knexfile.js",
    "db:status": "cd backend && npx knex migrate:status --knexfile ../database/knexfile.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "fintech",
    "credit",
    "blockchain",
    "p2p",
    "lending"
  ],
  "author": "QI Credit Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "knex": "^3.1.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "uuid": "^13.0.0"
  }
}
