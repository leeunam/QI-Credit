{
  "name": "qi-credit",
  "version": "0.1.0",
  "description": "",
  "main": "backend/server.js",
  "scripts": {
    "start": "node scripts/start.js",
    "validate": "node scripts/validate-all.js",
    "start:backend": "cd backend && npm run dev",
    "start:frontend": "cd frontend && npm run dev",
    "dev": "node scripts/start.js",
    "setup": "npm install && cd backend && npm install && cd ../frontend && npm install && cd .. && npm run db:migrate",
    "db:migrate": "cd backend && npx knex migrate:latest --knexfile ../database/knexfile.js",
    "db:rollback": "cd backend && npx knex migrate:rollback --knexfile ../database/knexfile.js",
    "db:status": "cd backend && npx knex migrate:status --knexfile ../database/knexfile.js"
  },
